  <!-- MiSans by KiriChen-->
  <family customizationType="new-named-family" name="misans">
    <font weight="100" style="thin">MiSans-Thin.ttf
        <axis tag="wght" stylevalue="100" />
    </font>
    <font weight="200" style="extralight">MiSans-ExtraLight.ttf
        <axis tag="wght" stylevalue="200" />
    </font>
    <font weight="300" style="light">MiSans-Light.ttf
        <axis tag="wght" stylevalue="300" />
    </font>
    <font weight="400" style="normal">MiSans-Normal.ttf
        <axis tag="wght" stylevalue="400" />
    </font>
    <font weight="500" style="medium">MiSans-Medium.ttf
        <axis tag="wght" stylevalue="500" />
    </font>
    <font weight="600" style="semibold">MiSans-Semibold.ttf
        <axis tag="wght" stylevalue="600" />
    </font>
    <font weight="700" style="bold">MiSans-Bold.ttf
        <axis tag="wght" stylevalue="700" />
    </font>
    <font weight="900" style="heavy">MiSans-Heavy.ttf
        <axis tag="wght" stylevalue="900" />
    </font>
  </family>
  <alias name="misans-light" to="misans" weight="100"/>
  <alias name="misans-normal" to="misans" weight="400"/>
  <alias name="misans-medium" to="misans" weight="500"/>